<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="http://www.w3.org/2010/09/qt-fots-catalog"
         xml:base="../qt3tests/catalog.xml"
    test-suite="FOTS" version="3.1">
    <environment name="empty">
        <!-- The context item MUST be undefined/absent for queries run in this environment. -->
        <!-- The test query will always be an expression, and if necessary an empty context can be achieved by wrapping
             the expression as the body of a function -->
    </environment>
    <environment name="works-mod">
        <source role="." file="docs/works-mod.xml">
            <description>Data for various NIST tests (abbreviated, unabbreviated syntax)</description>
            <created by="Carmelo Montanez" on="2005-03-04"/>
        </source>
    </environment>
    <environment name="works">
        <source role="." file="docs/works.xml">
            <description>Data for various NIST tests</description>
            <created by="Carmelo Montanez" on="2005-03-04"/>
        </source>
    </environment>
    <environment name="staff">
        <source role="." file="docs/staff.xml">
            <description>Data for various NIST tests</description>
            <created by="Carmelo Montanez" on="2005-03-04"/>
        </source>
    </environment>
    <environment name="works-and-staff">
        <source role="$works" file="docs/works.xml">
            <description>Data for various NIST tests</description>
            <created by="Carmelo Montanez" on="2005-03-04"/>
        </source>
        <source role="$staff" file="docs/staff.xml">
            <description>Data for various NIST tests</description>
            <created by="Carmelo Montanez" on="2005-03-04"/>
        </source>
    </environment>
    <environment name="map">
        <namespace prefix="map" uri="http://www.w3.org/2005/xpath-functions/map"/>
    </environment>
    <environment name="atomic">
        <namespace prefix="atomic" uri="http://www.w3.org/XQueryTest"/>
        <schema uri="http://www.w3.org/XQueryTest" file="docs/atomic.xsd">
            <description>A Schema for atomic.xml</description>
            <created by="Carmelo Montanez" on="2005-03-08"/>
        </schema>
        <source role="." file="docs/atomic.xml" validation="strict" uri="http://www.w3.org/fots/docs/atomic.xml">
            <description>A Schema validated xml file, that contains values for data types. Can be used by any test.</description>
            <created by="Carmelo Montanez" on="2005-03-08"/>
        </source>
    </environment>
